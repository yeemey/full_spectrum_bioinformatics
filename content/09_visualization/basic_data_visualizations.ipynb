    "#### Load Crow Count Data from Ute et al., 2018\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "crow_count_data = pd.read_csv(\"./resources/CC_Data.csv\",sep = \";\")\n",
    "crow_count_data = crow_count_data.set_index(\"Index\")\n",
    "crow_count_data.head()"
   ]
